<%- include ("partials/header"); -%>

<% posts.forEach(element=> {%>
   <h1> <%= element.title %></h1>
   <p>
      <%= element.content.substring(0,200) %>
      <% if(element.content.length>200) { %> 
      <a href="/posts/<%= element.id %>">... Read More</a>
      <%}%>
   </p>
<%}); %>  

<%- include ("partials/footer"); -%>